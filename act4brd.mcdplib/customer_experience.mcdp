mcdp {
    provides avg_travel_time [s]
    provides avg_discomfort_level [Reals]
    provides avg_transfers [Reals]
    provides avg_hops [Reals]

    requires dissatisfaction_level [NAt]

    weight_of_hops = 5 [Nat]
    weight_of_transfers = 10 [NAt]

    required dissatisfaction_level >= avg_travel_time * avg_discomfort_level + weight_of_hops * avg_hops + weight_of_transfers * avg_transfers
}
