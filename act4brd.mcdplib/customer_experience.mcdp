mcdp {
    provides avg_tt [s]
    provides discomfort [dimensionless]
    provides avg_transfers [dimensionless]
    provides avg_hops [dimensionless]

    requires dissatisfaction_level [dimensionless]

    weight_of_hops = 5 [dimensionless]
    weight_of_transfers = 10 [dimensionless]

    required dissatisfaction_level >= avg_tt * discomfort + weight_of_hops * avg_hops + weight_of_transfers * avg_transfers
}
